# Environment configuration for Opinion Admin UI

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api/v1
VITE_API_TIMEOUT=30000

# Enable Mock API for development (set to 'true' to use mock data, 'false' to use real API)
VITE_ENABLE_MOCK_API=true

# Debug and logging
VITE_ENABLE_DEBUG=true
VITE_ENABLE_LOGGING=true

# UI Configuration
VITE_ITEMS_PER_PAGE=25
VITE_MAX_UPLOAD_SIZE=10485760

# Company Information
VITE_SUPPORT_EMAIL=support@inqwise.com
VITE_COMPANY_NAME=Inqwise Ltd
